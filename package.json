{
  "name": "s4ft-framework",
  "version": "1.0.1",
  "private": false,
  "description": "**s4ft** (Simple And Fast Templates) is a declarative web development language and framework inspired by Next.js. It provides an intuitive syntax for building modern web applications with built-in routing, state management, and component architecture.",
  "keywords": [
    "framework",
    "s4ft",
    "javascript",
    "typescript",
    "frontend",
    "fullstack",
    "web-framework",
    "react",
    "nextjs-alternative",
    "brazilian-framework"
  ],
  "homepage": "https://s4ft.fun",
  "bugs": {
    "url": "https://github.com/s4ft-framework/s4ft/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/s4ft-framework/s4ft.git"
  },
  "license": "MIT",
  "author": "Marcos Dresbach",
  "main": "./dist/index.js",
  "bin": {
    "s4ft": "./dist/bin/s4ft.js"
  },
  "directories": {
    "lib": "lib",
    "example": "examples"
  },
  "scripts": {
    "build": "next build",
    "build-framework": "tsc",
    "cli": "node dist/bin/s4ft.js",
    "dev": "next dev",
    "format": "prettier --write src/**/*.ts",
    "postinstall": "node ./bin/postinstall.js",
    "lint": "next lint",
